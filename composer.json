{
    "name": "giudicelli/symfony-distributed-architecture",
    "type": "symfony-bundle",
    "description": "Symfony distributed architecture",
    "keywords": [],
    "homepage": "https://github.com/giudicelli/symfony-distributed-architecture",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Giudicelli"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-eio": "*",
        "psr/log": "~1.0",
        "giudicelli/distributed-architecture": ">=0.5.1",
        "giudicelli/distributed-architecture-queue": ">=0.4.0",
        "symfony/console": "^5",
        "symfony/config": "^5",
        "symfony/http-kernel": "^5",
        "symfony/dependency-injection": "^5",
        "symfony/framework-bundle": "^5",
        "symfony/yaml": "^5",
        "symfony/orm-pack": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "conflict": {
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "giudicelli\\DistributedArchitectureBundle\\": "./" },
        "exclude-from-classmap": [
            "/Tests/",
            "/bin/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    }
}
